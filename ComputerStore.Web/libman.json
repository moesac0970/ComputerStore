{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "microsoft-signalr@5.0.0-preview.1.20124.5",
      "destination": "wwwroot/microsoft-signalr/",
      "files": [
        "signalr.min.js",
        "signalr.js"
      ]
    },
    {
      "library": "aspnet-signalr@1.1.4",
      "destination": "wwwroot/lib/aspnet-signalr/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@3.1.2",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}